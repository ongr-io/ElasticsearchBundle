{
    "name": "handcraftedinthealps/elasticsearch-bundle",
    "description": "Elasticsearch bundle for Symfony.",
    "type": "symfony-bundle",
    "homepage": "http://ongr.io",
    "license": "MIT",
    "authors": [
        {
            "name": "ONGR team",
            "homepage": "http://www.ongr.io"
        },
        {
            "name": "Handcrafted in the Alps Team",
            "homepage": "https://github.com/handcraftedinthealps/ElasticsearchBundle/graphs/contributors"
        }
    ],
    "require": {
        "php": "^5.6|^7.0",
        "symfony/framework-bundle": "^2.8|^3.0|^4|^5",
        "symfony/console": "^2.8|^3.0|^4|^5",
        "symfony/stopwatch": "^2.8|^3.0|^4|^5",
        "symfony/templating": "^2.8|^3.0|^4|^5",
        "symfony/asset": "^2.8|^3.0|^4|^5",
        "doctrine/annotations": "~1.2",
        "doctrine/inflector": "~1.0",
        "doctrine/cache": "~1.4",
        "doctrine/collections": "~1.4",
        "monolog/monolog": "~1.10",
        "handcraftedinthealps/elasticsearch-dsl": "~5.0|~6.0|~7.0"
    },
    "require-dev": {
        "elasticsearch/elasticsearch": "~5.0|~6.0|~7.0",
        "mikey179/vfsstream": "~1.4",
        "squizlabs/php_codesniffer": "~2.0",
        "satooshi/php-coveralls": "~1.0",
        "symfony/browser-kit" : "^3.4|^4|^5",
        "symfony/expression-language" : "^3.4|^4|^5",
        "symfony/twig-bundle": "^3.4|^4|^5",
        "symfony/serializer": "^3.4|^4|^5",
        "symfony/yaml": "^3.4|^4|^5",
        "symfony/phpunit-bridge": "^5.1",
        "symfony/dependency-injection": "^3.4|^4|^5",
        "symfony/validator": "^3.4|^4|^5",
        "symfony/options-resolver": "^3.4|^4|^5",
        "symfony/var-dumper": "^3.4|^4|^5"
    },
    "autoload": {
        "psr-4": { "ONGR\\ElasticsearchBundle\\": "" },
        "exclude-from-classmap": ["/Tests/", "/Test/"]
    },
    "conflict": {
        "handcraftedinthealps/elasticsearch-dsl": "6.0 - 6.1.1 || 7.0 - 7.1.1",
        "ongr/elasticsearch-dsl": "6.0 - 6.1.1 || 7.0 - 7.1.1"
    },
    "replace": {
        "ongr/elasticsearch-bundle": "self.version"
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "5.2-dev"
        }
    }
}
