{
    "name": "ongr/elasticsearch-bundle",
    "description": "Elasticsearch bundle for Symfony.",
    "type": "symfony-bundle",
    "homepage": "http://ongr.io",
    "license": "MIT",
    "authors": [
        {
            "name": "ONGR team",
            "homepage": "http://www.ongr.io"
        }
    ],
    "require": {
        "php": "^7.0",
        "symfony/framework-bundle": "^3.0|^4.0",
        "symfony/console": "^3.0|^4.0",
        "symfony/stopwatch": "^3.0|^4.0",
        "symfony/templating": "^3.0|^4.0",
        "doctrine/annotations": "^1.6",
        "doctrine/inflector": "^1.3",
        "doctrine/cache": "~1.7",
        "doctrine/collections": "^1.5",
        "monolog/monolog": "^1.23",
        "elasticsearch/elasticsearch": "^6.0",
        "ongr/elasticsearch-dsl": "^6.0"
    },
    "require-dev": {
        "mikey179/vfsStream": "~1.6",
        "phpunit/phpunit": "^6.0",
        "squizlabs/php_codesniffer": "^3.0",
        "satooshi/php-coveralls": "^2.0",
        "symfony/browser-kit" : "^3.0|^4.0",
        "symfony/expression-language" : "^3.0|^4.0",
        "symfony/twig-bundle": "^3.0|^4.0",
        "symfony/serializer": "^3.0|^4.0",
        "symfony/yaml": "^3.0|^4.0",
        "symfony/dependency-injection": "^3.0|^4.0",
        "symfony/validator": "^3.0|^4.0",
        "symfony/options-resolver": "^3.0|^4.0"
    },
    "autoload": {
        "psr-4": { "ONGR\\ElasticsearchBundle\\": "" },
        "exclude-from-classmap": ["/Tests/", "/Test/"]
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "6.0-dev"
        }
    }
}
